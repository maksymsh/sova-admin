{
  "name": "sova/admin",
  "description": "The Sova Laravel Admin.",
  "keywords": [
    "sova", "laravel", "admin"
  ],
  "license": "MIT",
  "type": "library",
  "require": {
    "php": "^7.2",
    "laravel/framework": "5.6.*",
    "laravelcollective/html": "^5.6"
  },
  "require-dev": {},
  "autoload": {
    "psr-4": {
      "Sova\\Admin\\": "src/"
    },
    "files": [
      "src/Helpers/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Sova\\Admin\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sova\\Admin\\AdminServiceProvider"
      ],
      "aliases": {
        "Admin": "Sova\\Admin\\Facades\\Admin"
      }
    }
  }
}
